<!DOCTYPE node PUBLIC "-//freedesktop//DTD D-BUS Object Introspection 1.0//EN" "http://www.freedesktop.org/standards/dbus/1.0/introspect.dtd">
<node>
    <interface name="org.openobex.ServerSession">
        <method name="Accept">
        </method>
        <method name="Reject">
        </method>
        <method name="Disconnect">
        </method>
        <method name="GetTransferInfo">
            <arg type="o" direction="out"/>
        </method>
        <method name="Cancel">
        </method>
        <signal name="Cancelled">
        </signal>
        <signal name="Disconnected">
        </signal>
        <signal name="TransferStarted">
            <arg name="filename" type="s" direction="out"/>
            <arg name="local_path" type="s" direction="out"/>
            <arg name="total_bytes" type="t" direction="out"/>
        </signal>
        <signal name="TransferProgress">
            <arg name="bytes_transferred" type="t" direction="out"/>
        </signal>
        <signal name="TransferCompleted">
        </signal>
        <signal name="ErrorOccurred">
            <arg name="error_name" type="s" direction="out"/>
            <arg name="error_message" type="s" direction="out"/>
        </signal>
        <signal name="RemoteDisplayRequested">
            <arg name="filename" type="s" direction="out"/>
        </signal>
    </interface>
</node>